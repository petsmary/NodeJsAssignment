
 <h2 class="page-header">Message</h2>
<div class="container">
    <table id="example" class="table table-striped table-bordered" style="width:100%">
      <thead>
        <tr>
          <th>Post Date</th>
          <th>Title</th>
          <th>Your Message</th>
          <th>Feedback</th>
        </tr>
      </thead>
      <tbody>
        {{#each contacts}}
        <tr>
          <td>{{this.post_date}}</td>
          <td>{{this.subject}}</td>
          <td>{{this.message}}</td>
          {{#each this.feedback}}
          <td>{{this.body}} <br>{{this.date}}</td>
          {{/each}}
        </tr>
        {{/each}}
      </tbody>
      <tfoot>
        <th>Post Date</th>
          <th>Title</th>
          <th>Your Message</th>
          <th>Feedback</th>
      </tfoot>
    </table>

<br>
</div>
<script>
  $(document).ready(function() {
    $('#example').DataTable();
} );
</script>