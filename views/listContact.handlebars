
 <h2 class="page-header">Contact List</h2>


 <div class="container">

<p></p>
    <table id="example" class="display" style="width:100%">
      <thead>
        <tr>
          <th>Post Date</th>
          <th>Name</th>
          <th>Subject</th>
          <th>Message</th>
          <th>Phone Number</th>
          <th>Email</th>
          <th>Username</th>
          <th>Action</th>
        </tr>
      </thead>
      <tbody>
        {{#each contacts}}
        <tr>
            <td>{{this.post_date}}</td>
          <td>{{this.name}}</td>
          <td>{{this.subject}}</td>
          <td>{{this.message}}</td>
          <td>{{this.phone}}</td>
          <td>{{this.email}}</td>
          <td>{{this.username}}</td>
          <td><button class="btn btn-default">feedback</button>
          </tr>
        {{/each}}
      </tbody>
      <tfoot>
        <th>Post Date</th>
          <th>Name</th>
          <th>Subject</th>
          <th>Message</th>
          <th>Phone Number</th>
          <th>Email</th>
          <th>Username</th>
          <th>Action</th>
      </tfoot>
    </table>
</div>
<br>
<script>
  $(document).ready( function () {
			$('#example')
				.addClass( 'nowrap' )
				.dataTable( {
					responsive: true,
					columnDefs: [
						{ targets: [-1, -3], className: 'dt-body-right' }
					]
				} );
		} );
</script>
